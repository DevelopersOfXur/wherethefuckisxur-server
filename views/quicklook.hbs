<div class="data-box-container">
    <div class="spider-home white-box data-box">
        <h3><a href="/data/vendor/863940356">The Spider</a></h3>
        <p class="flavor-text">Big Fat Fallen</p>
        <ul id="spiderinv">
            {{#each spiderinventory}}
            <li>
                <span class="item">{{name}}</span><br />
                <span class="item-description">{{cost}}</span>
            </li>
            {{/each}}
        </ul>
    </div>

    <div class="banshee white-box data-box">
        <h3><a href="/data/vendor/672118013">Banshee Mods</a></h3>
        <p class="flavor-text">Where'd I put those parts</p>
        <ul id="bansheeinv">
            {{#each bansheeinventory}}
            <li>
                <span class="item">{{name}}</span><br />
                <span class="item-description">{{desc}}</span>
            </li>
            {{/each}}
        </ul>
    </div>

    <div class="dailies white-box data-box">
        <h3>Dailies</h3>
        <p class="flavor-text"></p>
        <ul id="dailies">
            <li class="dailies-today item">{{dailies.current}}</li>
            <li class="dailies-and-then">And then...</li>
            {{#each dailies.next}}
            <li class="dailies-future sub-item">{{daily}}</li>
            {{/each}}
        </ul>
    </div>

    <div class="white-box data-box">
        <h3>Active Nightfalls</h3>
        <p class="flavor-text"></p>
        <ul id="nightfalls">
            {{#each activenightfalls}}
            <li class="nightfall item">{{this}}</li>
            {{/each}}
        </ul>
    </div>
</div>