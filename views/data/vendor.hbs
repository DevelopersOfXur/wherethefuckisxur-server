{{!< data}}

<div class="vendor-info">
    <div class="vendor-name">
        {{display.name}}
    </div>
    <div class="vendor-dropdown">
        {{#each vendors}}
        <a class="vendor-option" href="/data/vendor/{{hash}}">
            {{name}}
        </a>
        {{/each}}
    </div>
    <p class="vendor-desc">
        {{desc}}
    </p>
</div>
<div class="vendor-sales">
    {{#each categories}}
    {{> vendorcat}}
    {{/each}}
</div>
