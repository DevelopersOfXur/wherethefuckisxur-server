{{!< data}}

<div class="data-box-container">
    <div class="white-box data-box">
        <h2>Ascendant Challenge</h2>
        <p><h3 class="flavor-text">{{ascendantchallenge.name}}</h3></p>
        <a href="/data/guides/ascendantchallenge#ac{{ascendantchallenge.id}}">Guide</a>
    </div>

    <div class="white-box data-box">
        <h2>Escalation Protocol</h2>
        <p>Boss:</p>
        <p><h3 class="flavor-text">{{escalationprotocol.boss}}</h3></p>
        <p>Gun:</p>
        <p><h3 class="flavor-text">{{escalationprotocol.gun}}</h3></p>
        <a href="/data/guides/escalationprotocol#ep{{escalationprotocol.id}}">Guide</a>
    </div>

    <div class="white-box data-box">
        <h2>The Dreaming City</h2>
        <p>Curse Level:</p>
        <p class="flavor-text"><h3>{{citystatus.status}}</h3></p>
        <p>Blind Well Boss</p>
        <p class="flavor-text"><h3>{{citystatus.boss}}</h3></p>
        <a href="/data/guides/blindwell#bw{{citystatus.id}}">Boss Guide</a>
    </div>

    <div class="white-box data-box">
        <h2>Dailies</h2>
        <p class="flavor-text"></p>
        <ul id="dailies">
            <li class="dailies-today item">{{dailies.current}}</li>
            <li class="dailies-and-then">And then...</li>
            {{#each dailies.next}}
            <li class="dailies-future sub-item">{{this}}</li>
            {{/each}}
        </ul>
    </div>

    <div class="white-box data-box">
        <h2>Active Nightfalls</h2>
        <p class="flavor-text"></p>
        <ul id="nightfalls">
            {{#each activenightfalls}}
            <li class="nightfall item">{{this}}</li>
            {{/each}}
        </ul>
    </div>
</div>